#!/home/sysadmin/energydata/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'datacats','console_scripts','datacats-lesscd'
__requires__ = 'datacats'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('datacats', 'console_scripts', 'datacats-lesscd')()
    )
